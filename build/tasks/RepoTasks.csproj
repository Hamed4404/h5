<Project Sdk="Microsoft.NET.Sdk">
  <Import Project="$(RepoTasksSdkPath)\Sdk.props" Condition="'$(RepoTasksSdkPath)' != '' "/>

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NuGet.Packaging" Version="$(NuGetInMSBuildVersion)" />

    <!-- See https://aka.ms/esrpclient -->
    <PackageReference Include="ESRPClient" Version="$(ESRPClientPackageVersion)" />

    <!-- See https://devdiv.visualstudio.com/DevDiv/_packaging?feed=WebTools&package=SignChec&protocolType=NuGet&_a=package  -->
    <PackageReference Include="SignCheck" Version="$(SignCheckVersion)" />
   </ItemGroup>

  <Import Project="$(RepoTasksSdkPath)\Sdk.targets" Condition="'$(RepoTasksSdkPath)' != '' "/>

</Project>
