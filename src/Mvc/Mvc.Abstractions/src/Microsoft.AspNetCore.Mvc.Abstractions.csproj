<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>ASP.NET Core MVC abstractions and interfaces for action invocation and dispatching, authorization, action filters, formatters, model binding, routing, validation, and more.
Commonly used types:
Microsoft.AspNetCore.Mvc.IActionResult</Description>
    <TargetFramework>$(DefaultNetCoreTargetFramework)</TargetFramework>
    <IsAspNetCoreApp>true</IsAspNetCoreApp>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageTags>aspnetcore;aspnetcoremvc</PackageTags>
    <IsPackable>false</IsPackable>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="$(SharedSourceRoot)ClosedGenericMatcher\*.cs" />
    <Compile Include="$(SharedSourceRoot)\ParameterBindingMethodCache.cs" Link="ModelBinding\ParameterBindingMethodCache.cs" />
    <Compile Include="$(SharedSourceRoot)\TypeNameHelper\TypeNameHelper.cs" Link="ModelBinding\TypeNameHelper.cs" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Microsoft.AspNetCore.Routing.Abstractions" />
    <Reference Include="Microsoft.Net.Http.Headers" />
  </ItemGroup>

  <ItemGroup>
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.ApiExplorer" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.Core" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.Cors" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.DataAnnotations" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.Formatters.Xml" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.Localization" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.Razor" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.RazorPages" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.TagHelpers" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.Testing" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.ViewFeatures" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.Abstractions.Test" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.ApiExplorer.Test" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.Core.Test" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.Core.TestCommon" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.Cors.Test" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.DataAnnotations.Test" />
    <InternalVisibleTo Include="Microsoft.AspNetCore.Mvc.Formatters.Xml.Test" />
