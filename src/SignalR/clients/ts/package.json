{
  "name": "client-ts",
  "version": "1.0.0",
  "description": "Common dependencies used during dev time. DO NOT PUBLISH",
  "private": true,
  "workspaces": [
    "signalr",
    "signalr-protocol-msgpack",
    "FunctionalTests"
  ],
  "scripts": {
    "build": "npm run --prefix ./signalr build && npm run --prefix ./signalr-protocol-msgpack build",
    "lint-signalr": "eslint -c ./signalr/tests/.eslintrc.js --ext .ts ./signalr/tests ",
    "lint-signalr-msgpack": "eslint -c ./signalr-protocol-msgpack/tests/.eslintrc.js --ext .ts ./signalr-protocol-msgpack/tests ",
    "pretest": "npm run lint-signalr & npm run lint-signalr-msgpack",
    "test": "jest --config ./jest.config.js",
    "coverage": "jest --config ./jest.config.js --coverage"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.16.0",
    "eslint-plugin-jsdoc": "^44.2.3",
    "inspectpack": "^4.6.1",
    "jest": "^29.5.0",
    "jest-junit": "^13.0.0",
    "jest-jasmine2": "^29.5.0",
    "rimraf": "^3.0.2",
    "terser": "^5.14.2",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^29.0.5",
    "ts-loader": "^8.0.17",
    "typescript": "^4.9.5",
    "webpack": "^5.76.3",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {},
  "resolutions": {
    "ansi-regex": "5.0.1",
    "set-value": ">=4.0.1",
    "minimist": ">=1.2.6",
    "string_decoder": ">=1.3.0"
  },
  "author": "Microsoft",
  "license": "MIT"
}
