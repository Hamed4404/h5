    </When>
    <!-- VB specific settings -->
    <When Condition="'$(Language)' == 'VB'">
      <PropertyGroup>
        <MyType>Empty</MyType>
        <OptionCompare>Binary</OptionCompare>
        <OptionStrict>On</OptionStrict>
        <RemoveIntegerChecks>true</RemoveIntegerChecks>
